html
    head
        title= 'Nautilus'
        link(rel='stylesheet', href='stylesheets/webShell.css')
    body
        input(hidden, id='clientDomain', value=clientDomain)
        div(class='title-bar')
            div(class='title-bar-text')
                h2= 'Nautilus'
                span= 'Administering: ' + clientDomain
        div(class='command-output', id='out')
        div(class='command-input', id='in')
            span(id='command-label') ps>
            input(id='command', type='text', autofocus)
        script(type="text/javascript").
            const clientDomain = document.getElementById('clientDomain').value;
        script(src='/socket.io/socket.io.js')
        script(src='https://code.jquery.com/jquery-1.11.1.js')
        script(src='scripts/webShell.js')